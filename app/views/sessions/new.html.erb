<div class="row">
  <div class="col"></div>
  <div class="col-4">
    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :username %>
      <%= f.text_field :username, class: 'form-control' %>

      <%= f.submit 'Sign in', class: 'btn btn-primary' %> Not a user?, <%= link_to  "Sign up", new_user_path %>
    <% end %>
  </div>
  <div class="col"></div>
</div>
