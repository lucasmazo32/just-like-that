<div class="grid-sis">
  <%= render 'layouts/nav' %>
  <%= render 'layouts/profile_menu' %>
  <div class="main-body">
    <%= foreign_view(@user) %>
    <h4><%= @user.name %><%= follow_or_stop(@user) %></h4>
    <p><%= link_to  "Edit", edit_user_path(@user), class:'btn btn-primary' %></p>
    <span><%= delete_user?(@user) %></span>
  </div>
</div>